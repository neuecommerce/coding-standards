{
    "name": "neuecommerce/coding-standards",
    "description": "Coding standards for Neue Commerce projects.",
    "type": "composer-plugin",
    "keywords": [
        "php",
        "neuecommerce",
        "coding-standards"
    ],
    "homepage": "https://neuecommerce.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Neue Commerce Team",
            "email": "hello@neuecommerce.com",
            "homepage": "https://neuecommerce.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "composer-plugin-api": "^2.1",
        "composer/composer": "^2.8",
        "larastan/larastan": "^3.2",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-mockery": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "rector/rector": "^2.0",
        "symplify/easy-coding-standard": "^12.5"
    },
    "autoload": {
        "psr-4": {
            "NeueCommerce\\CodingStandards\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "process-timeout": 0,
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "class": "NeueCommerce\\CodingStandards\\Plugin"
    },
    "minimum-stability": "stable"
}
